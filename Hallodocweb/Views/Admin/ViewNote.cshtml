@using DataAccess.CustomModel;@model ViewNotesViewModel;@{    ViewData["Title"] = "ViewNote";}<link rel="stylesheet" href="~/css/ViewNote.css" /><partial name="_Navbar"/><div class="container-fluid d-flex justify-content-center row ">    <div class="col-8">        <div class="d-flex justify-content-between mt-3">            <div class="d-flex">                <h4><strong>View Notes</strong></h4>            </div>            <a asp-controller="Admin" asp-action="AdminDashboard" class="btn btn-outline-info shadow-none night-mode-btn d-flex align-items-center">                <svg xmlns="http://www.w3.org/2000/svg" width="15" height="15" viewBox="0 -960 960 960">                    <path d="m142-480 294 294q15 15 14.5 35T435-116q-15 15-35 15t-35-15L57-423q-12-12-18-27t-6-30q0-15 6-30t18-27l308-308q15-15 35.5-14.5T436-844q15 15 15 35t-15 35L142-480Z" />                </svg>                <span>Back</span>            </a>        </div>        <form method="post" asp-action="UpdateNotes" asp-controller="Admin">            <div class="shadow p-3 dashboard-card mt-4">                <hr class="mt-3 mb-0" />                <div class="row">                    <div class="col-md-6 col-12">                        <div class="form-floating mt-3 notes d-flex">                            <img src="~/img/transfer.svg" class="svg mx-2" />                            Transfer Notes :                             <span class="mx-2">@Model.TransferNotes</span>                        </div>                    </div>                    <div class="col-md-6 col-12">                        <div class="form-floating mt-3 notes">                            <img src="~/img/doctor.svg" class="svg mx-2" />                            Physician Notes                            <span>@Model.PhysicianNotes</span>                        </div>                    </div>                    <div class="col-md-6 col-12">                        <div class="form-floating mt-3 notes">                            <img src="~/img/admin.svg" class="svg mx-2" />                            Admin Notes                            <span>@Model.AdminNotes</span>                        </div>                    </div>                    <div class="row">                        <div class="col-12">                            <div class="form-floating mt-3">                                <textarea style="height:100px" class="form-control" placeholder="Leave a comment here" id="floatingTextarea2" asp-for="AdditionalNotes"></textarea>                                <label for="floatingTextarea2">Admin Notes                                    
                                </label>                            </div>                        </div>                    </div>                    <div class="d-flex justify-content-end mt-3 gap-1">                        <button type="submit" class="btn btn-primary">Save Changes</button>                    </div>                </div>            </div>        </form>    </div></div>