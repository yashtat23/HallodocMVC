@using DataAccess.CustomModel;@model SendAgreement;<div class="modal fade" id="sendagreementmodel" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">    <div class="modal-dialog modal-dialog-centered">        <div class="modal-content border border-0 rounded-3">            <form  method="post" asp-controller="Admin" asp-action="SendAgreement">                <div class="modal-header bg-info text-white">                    <h5 class="modal-title" id="staticBackdropLabel">Send Agreement</h5>                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>                </div>                <div class="modal-body">                    <div class="d-flex flex-column">                                                <div class="text-secondary mt-2 mb-2 mr-1">To Send Agreement please make sure your are updating the correct contact information below for thr responsible party.</div>                    </div>                    <div class=" gap-2">                        <div class="form-floating mb-3">                            <input asp-for="@Model.phonenumber" type="tel" class="form-control" id="floatingInput1" placeholder="phone number" />                            <label for="floatingInput1" class="">Phone Number</label>                        </div>                        <div class="form-floating mb-3">                            <input asp-for="@Model.email" type="email" class="form-control" id="floatingInput" mailto:placeholder="abc@gmail.com" />                            <label for="floatingInput" class="">Email</label>                        </div>                    </div>                </div>                <div class="modal-footer">                    <button type="submit" class="btn bg-info px-4 text-white">Send</button>                    <button type="button" class="btn border border-info text-info" data-bs-dismiss="modal">Cancel</button>                </div>            </form>        </div>    </div></div>