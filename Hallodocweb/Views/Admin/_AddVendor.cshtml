@using DataAccess.CustomModel;@model AddBusinessModel;<div class="container">    <div class=" d-flex justify-content-between mt-4">         <h4>            Submit Information        </h4>                    <a  class="btn btn-primary m-2 text-decoration-none float-end" id="back-btn">                Back            </a>             </div>     <div class=" d-flex justify-content-center mt-4">        <div class="viewCaseForm form-floating shadow patient-request-form p-3 w-100">            <form method="post" asp-controller="Admin" asp-action="AddBusiness">                               <div class="row g-2 m-1">                    <div class="col-md">                        <div class="form-floating">                            <input type="text" class="form-control" id="businessName"  asp-for="BusinessName" placeholder=""/>                             <label for="userName">Business Name</label>                        </div>                    </div>                    <div class="col-md">                        <div class="form-floating">                            <select class="form-select mb-3" id="role" asp-for="ProfessionId">                                                                <option selected disabled>Select A Profession</option>                                 @foreach (var item in Model.ProfessionList)
                                {                                    <option value="@item.Healthprofessionalid">@item.Professionname</option>                                }                             </select>                            <label for="role">Select A Profession</label>                        </div>                    </div>                </div>                <div class="row g-2 m-1">                    <div class="col-md-6">                        <div class="form-floating">                            <input type="text" class="form-control" id="faxNumber" asp-for="FaxNumber" placeholder=""/>                             <label for="userName">Fax Number</label>                        </div>                    </div>                    <div class="col-md-6">                        <div class="form-floating">                            <input type="tel" class="form-control" asp-for="PhoneNumber" placeholder="">                             <label for="">Phone Number</label>                        </div>                    </div>                </div>                <div class="row g-2 m-1">                    <div class="col-md-6">                        <div class="form-floating">                            <input type="email" class="form-control" asp-for="Email" placeholder="">                            <label for="">Email</label>                        </div>                    </div>                    <div class="col-md-6">                        <div class="form-floating phone-div w-100 h-100">                            <input type="tel" class="patientPhone form-control w-100 h-100" asp-for="BusinessContact" placeholder="">                             <label for="">Business Contact</label>                        </div>                    </div>                </div>                <div class="row g-2 m-1">                    <div class="col-md-6">                        <div class="form-floating">                            <input type="text" class="form-control" asp-for="Street" placeholder="">                             <label for="">Street</label>                        </div>                    </div>                    <div class="col-md-6">                        <div class="form-floating">                            <input type="text" class="form-control" asp-for="City" placeholder="">                              <label for="">City</label>                        </div>                    </div>                </div>                <div class="row g-2 m-1">                    <div class="col-md-6">                        <div class="form-floating phone-div w-100 h-100 ">                            <select class="form-select border-0" aria-label="Default select example" asp-for="RegionId">                                                                <option selected disabled>Select A Region</option>                                 @foreach (var items in Model.RegionList)
                                {                                    <option value="@items.Regionid">@items.Name</option>                                }                             </select>                            <label for="">Select A Region</label>                        </div>                    </div>                    <div class="col-md-6">                        <div class="form-floating">                            <input type="text" class="form-control" asp-for="Zip" placeholder="">                            <label for="">Zip/Postal</label>                        </div>                    </div>                </div>                <button type="submit" class="btn btn-success float-end m-1 ">Save</button><br />            </form>        </div>    </div></div>