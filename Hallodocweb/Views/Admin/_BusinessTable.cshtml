@* @model PaginatedList<BusinessTable>; *@@using DataAccess.CustomModel;@model List<BusinessTable>;<table class="table table-hover table-responsive">    <thead>        <tr>            <td>Profession</td>            <td>Business Name</td>            <td>Email</td>            <td>Fax Number</td>             <td>Phone Number</td>             <td>Business Contact</td>            <td>Actions</td>        </tr>    </thead>    <tbody>        @foreach (var item in Model)
        {            <tr>                <td>@item.ProfessionId</td>                <td>@item.BusinessName</td>                <td>@item.Email</td>                <td>@item.FaxNumber</td>                <td>@item.PhoneNumber</td>                <td>@item.BusinessContact</td>                <td>                    <button class="btn btn-outline-info">Edit</button>                    <button class="btn btn-outline-info">Delete</button>                </td>            </tr>        }    </tbody></table>