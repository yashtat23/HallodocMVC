@{    ViewData["Title"] = "Order";
    // Layout = "~/Views/Shared/_AdminDashboardLayout.cshtml";}@using DataAccess.CustomModel;@model Order;<Partial name="_Navbar" /><div class="container">    <div class="d-flex justify-content-between pt-3">        <h4>Send Order</h4>        <a asp-area="" asp-action="AdminDashboard" class="btn border-2 border-info text-info p-2 px-3 fw-bold">            &lt;  Back        </a>    </div>    <div class="mt-3 border rounded-3 shadow pt-3 px-3">        <div class="row">            <div class="col-md-6">                <div class="form-floating mb-3">                    <select class="form-select text-secondary py-0" aria-label="Floating label select example" id="Healthprofessionalid">                        <option selected disabled>Select Profession</option>                        @foreach (var items in Model.Profession)
                        {                            <option value="@items.Healthprofessionalid">@items.Professionname </option>                        }                    </select>                    <label for="floatingSelect"></label>                </div>            </div>            <div class="col-md-6">                <div class="form-floating mb-3">                    <select class="form-select text-secondary py-0" id="Vendorid" aria-label="Floating label select example">                        <option selected disabled>Business</option>                         @foreach (var items in Model.Business)
                        {                             <option value="@items.Vendorid">@items.Vendorname</option>                         }                     </select>                    <label for="floatingSelect"></label>                </div>            </div>            <div class="col-md-6">                <div class="form-floating mb-3">                    <input type="text" class="form-control" id="floatingInput" placeholder="business contact" />                    <label for="floatingInput" class="">Business Contact</label>                </div>            </div>            <div class="col-md-6">                <div class="form-floating mb-3">                    <input asp-for="email" type="email" class="form-control" id="email" mailto:placeholder="abc@gmail.com" />                    <label for="floatingInput" class="">Email</label>                </div>            </div>            <div class="col-md-6">                <div class="form-floating mb-3">                    <input type="text" class="form-control" id="floatingInput" placeholder="fax number" />                    <label for="floatingInput" class="">Fax Number</label>                </div>            </div>            <div class="col-12">                <div class="form-floating mb-3">                    <textarea class="form-control" placeholder="order details" id="floatingTextarea2" style="height: 100px"></textarea>                    <label for="floatingTextarea2" class="">                        Prescription or Order details                    </label>                </div>            </div>            <div class="col-md-6">                <div class="form-floating">                    <select class="form-select" id="floatingSelect" aria-label="Floating label select example">                        <option selected>Not Required</option>                        <option value="1">One</option>                        <option value="2">Two</option>                        <option value="3">Three</option>                    </select>                    <label for="floatingSelect">Number Of Refil</label>                </div>            </div>        </div>        <div class="footer mt-5 mb-4 text-end">            <button type="submit" class="btn bg-info text-white">Submit</button>            <button type="button" class="btn border border-info text-info" data-bs-dismiss="modal">Cancel</button>        </div>    </div>    <br />    <br />    <br /></div><script>          $('#Healthprofessionalid').on('change', function () {        let selectedValue = $('option:selected', this).val();        alert(selectedValue);        $.ajax({                            url: '/Admin/FetchVendors',            method: 'GET',            data: { selectedValue: selectedValue },            success: function (response) {                $('#Vendorid').empty();                console.log(response);                $.each(response, function (index, object) {                    $('#Vendorid').append($('<option>', {                        value: object["businessId"],                        text: object["businessName"]                    }));                });            },            error: function (error) {                console.error('Error fetching data: ' + error);            }        });    });   </script>